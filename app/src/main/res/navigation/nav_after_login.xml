<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/navigation_main">


    <activity
        android:id="@+id/mainActivity"
        android:name="com.seoultech.mobileprogramming.high_five.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment android:id="@+id/directMessageFragment" >
        <action
            android:id="@+id/navigation_dm"
            app:destination="@id/mainActivity" />
    </fragment>
    <fragment
        android:id="@+id/navigation_main"
        android:name="com.seoultech.mobileprogramming.high_five.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
        <action
            android:id="@+id/action_mainFragment_to_mainActivity2"
            app:destination="@id/mainActivity" />
    </fragment>
    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.seoultech.mobileprogramming.high_five.ui.dashboard.DashboardFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_dashboardFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
        <action
            android:id="@+id/action_dashboardFragment_to_mainActivity2"
            app:destination="@id/mainActivity" />
    </fragment>
    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.seoultech.mobileprogramming.high_five.ui.settings.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" >
        <action
            android:id="@+id/action_settingsFragment_to_mainActivity2"
            app:destination="@id/mainActivity" />
    </fragment>
</navigation>